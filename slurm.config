singularity.enabled = true
process {
    withName:featurecounts {
        executor='slurm'
        queue="${params.queue}"
        cpus=16
        mem='40 GB'
        time='6h'
        jobName={ "$task.name" }
        container = "${params.image_folder}subread-2.0.3.sif"
        containerOptions = "-B ${params.project_folder}:/workdir -B ${params.kallisto_raw_data}:/raw_data"
    }
}

